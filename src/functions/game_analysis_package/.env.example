# Game Analysis Package Configuration
# Copy this file to the project root .env and configure with your values

# Database Configuration (required for all modules)
SUPABASE_URL=your_supabase_url_here
SUPABASE_KEY=your_supabase_key_here

# Logging Configuration
LOG_LEVEL=INFO  # DEBUG for detailed logging

# Module-specific Configuration
# Player ID standardization (recommended)
PLAYER_ID_STANDARDIZATION_ENABLED=true
PLAYER_ID_PRIMARY_SYSTEM=gsis
PLAYER_ID_MAPPING_CACHE_TTL=3600
PLAYER_ID_ROSTER_FALLBACK_ENABLED=true
PLAYER_ID_SUPABASE_IN_BATCH_SIZE=150
